<%= form_for @user, url: signin_path(@user) do |f| %>
    <%= f.collection_select :name, User.all, :name, :name, prompt: "Please select your username from this dropdown" %><br><br>
    <%= f.label :password, 'Your password:' %><br>
    <%= f.password_field :password %><br>
    <%= f.submit 'Sign In' %>
<% end %>