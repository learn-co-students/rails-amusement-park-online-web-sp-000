<h1>Sign In</h1>

<%= form_tag(sessions_path) do %>
  Username: <%= text_field_tag :name %><br>
  Password: <%= password_field_tag :password %><br>
  Password Confirmation: <%= password_field_tag :password_confirmation %><br>
  <%= submit_tag "Signin" %>
<% end %><br>

<%= link_to "Sign Up", "/users/new" %>
