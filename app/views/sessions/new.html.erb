<h1>This is the signin page</h1>
<%= form_with(url: signin_path, method: "post") do %>
    <%= label_tag(:name, "Name:") %>
    <%= collection_select(:user, :name, User.all, :name, :name) %>  
    <%= label_tag(:password, "Password:") %>
    <%= password_field_tag(:password) %>
    <%= submit_tag("Sign In") %>
  <% end %>
