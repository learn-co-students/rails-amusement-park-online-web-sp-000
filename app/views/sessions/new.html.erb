<%= form_for @user, url: signin_path, method: :post do |f|%>
    <%= f.label :name, "Name: " %>
    <%= f.collection_select :name, User.all, :id, :name, prompt: "Choose your name"%> <br>
    <br>
    <%= f.label :password, "Password: " %>
    <%= f.password_field :password, id: 'password' %>
    <br>
    <%= f.submit "Sign In" %>
<% end %>
