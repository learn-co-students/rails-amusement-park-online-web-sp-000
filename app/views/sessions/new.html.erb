<%= form_tag ({controller: 'sessions', action: 'create', method: 'post'}) do %>
    <% @users.each do |user| %>
    Name: <%= select_tag "user[name]", user.name %>
    <%# debugger %>
    <% end %><br><br>
    Password: <%= password_field_tag "user[password]" %>
