<%= form_for :user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br><br>
  <%= f.label 'Select User' %>
  <%= f.collection_select :name, User.all, :name, :name %><br><br>
  <%= f.label :password %>
  <%= password_field_tag 'password' %><br><br>

  <%= f.submit 'Sign In' %>
<% end %>
