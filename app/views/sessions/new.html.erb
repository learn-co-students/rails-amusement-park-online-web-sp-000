<h1>Sign In:</h1>

<%= form_tag "/signin", method: "post" do %>
  <%= label_tag "Name:" %>
  <!--  <%= text_field_tag "name" %> -->
  <%= collection_select(:user, :name, User.all, :name, :name) %>

  <%= label_tag "Password:" %>
  <%= password_field_tag "password" %>

  <%= submit_tag "Sign In" %>
<% end %>
