<%= form_for @user do |f| %>
<div class="form-group">
  <%= f.label 'Your name: ' %> 
  <%= f.collection_select(:name, User.all, :id, :name) %>
</div>
<div class="form-group">
  <%= f.label 'Your Password: ' %> <br>
  <%= f.password_field :password, class: "form-control" %> <br>

<%= f.submit "Sign In", class: "btn btn-primary m-5" %>
</div>
<% end %>