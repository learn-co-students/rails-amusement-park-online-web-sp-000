<%= form_with(url: "/session", method: "post") do %>
<div class="form-group">
  <%= label_tag 'Your name: ' %> 
  <%= collection_select(:user, :name, User.all, :name, :name) %>
</div>
<div class="form-group">
  <%= label_tag 'Your Password: ' %> <br>
  <%= password_field_tag(:password, nil, class:"form-control") %> <br>

<%= submit_tag "Sign In", class: "btn btn-primary m-5" %>
</div>
<% end %>