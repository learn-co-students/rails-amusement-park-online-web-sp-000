<%= form_for(@attraction) do |f| %>
    <%= f.text_field :name, placeholder: "Name" %><br>
    <%= f.number_field :tickets, placeholder: "Tickets" %><br>
    <%= f.number_field :nausea_rating, placeholder: "Nausea Rating" %><br>
    <%= f.number_field :happiness_rating, placeholder: "Happiness Rating" %><br>
    <%= f.number_field :min_height, placeholder: "Min Height" %><br>
    <%= f.submit %>
<% end %>
