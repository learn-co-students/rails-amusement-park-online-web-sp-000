<ul>
  <% @attractions.each do |attraction| %>
    <li>
    <% if @user %>
      <%= link_to "Go on #{attraction.name}", attraction_path(attraction) %></li>
    <% else %>
      <%= attraction.name %>
    <% end %>
  <% end %>
</ul>

<% if !@user %>
  <%= link_to 'Sign Up', new_user_path %> or <%= link_to 'Sign In', signin_path %> to go on rides!
<% end %>
