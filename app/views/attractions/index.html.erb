<% @attractions.each do |attraction| %>
    <%= attraction.name %><br>
    Tickets: <%= attraction.tickets %><br>
    <% if @admin %>
        <%= link_to "Show #{attraction.name}", attraction %>
    <% else %>
        <%= link_to "Go on #{attraction.name}", attraction %>
    <% end %>
<br><br>
<% end %>

<% if @admin %>
    <%= link_to "New Attraction", '/attractions/new' %>
<% end %>