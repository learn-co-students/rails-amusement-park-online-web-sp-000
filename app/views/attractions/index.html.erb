<h1>Listing Attractions</h1>
<table>
<tr>
    <th>Name</th>
    <th>Tickets</th>
    <th>Details</th>
</tr>
<% @attractions.each do |attr| %>
    <tr>
        <td><%= attr.name %></td>
        <td><%= attr.tickets %></td>
        <td><%= attraction_link(attr) %></td>
    </tr>
<% end %>
</table>
<br>
<%= link_to "New Attraction", new_attraction_path %>