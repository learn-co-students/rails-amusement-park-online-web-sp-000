<ol>
<% @attractions.each do |att|%>
<li> <% if current_user.admin %>
          <%= link_to "Show #{att.name}", attraction_path(att) %>
        <% else %>
          <%= link_to "Go on #{att.name}", attraction_path(att) %>
        <% end %></li>

<%end%>
</ol>

<% if @user %>
  <p>
    <%= link_to "Back to profile", user_path(@user) %>
  </p>
<% else %>
  <p><%= link_to 'New Attraction', new_attraction_path %></p>
<% end %>