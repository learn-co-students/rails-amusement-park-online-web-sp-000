<%= form_with model: @attraction do |f| %> 
    <%= f.label :name, "Name" %>
    <%= f.text_field :name %><br>
    <%= f.label :min_height, "Minimum Height" %>
    <%= f.number_field :min_height %><br>
    <%= f.label :happiness_rating, "Happiness Rating" %>
    <%= f.number_field :happiness_rating %><br>
    <%= f.label :nausea_rating, "Nausea Rating" %>
    <%= f.number_field :nausea_rating %><br>
    <%= f.label :tickets, "Number of Tickets" %>
    <%= f.number_field :tickets %><br>

    <%= f.submit "Create Attraction" %>
<% end %>