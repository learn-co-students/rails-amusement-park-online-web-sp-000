<%= form_for @attraction do |f| %>
    Name: <%= f.text_field :name %>
    Min Height: <%= f.text_field :min_height %>
    Happiness Rating: <%= f.text_field :happiness_rating %>
    Nausea Rating: <%= f.text_field :nausea_rating %>
    Tickets: <%= f.text_field :tickets %>
    <%= submit_tag 'Update Attraction' %>
<% end %>