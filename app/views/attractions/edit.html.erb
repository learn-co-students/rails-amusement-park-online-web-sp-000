<%= form_for @attraction do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br><br>
  <%= f.label "Height (in inches)" %><br>
  <%= f.number_field :min_height %><br><br>
  <%= f.label "How happy are you? (1-5 scale)" %><br>
  <%= f.number_field :happiness_rating %><br><br>
  <%= f.label "How nauseous are you? (1-5 scale)" %><br>
  <%= f.number_field :nausea_rating %><br><br>
  <%= f.label "How many tickets do you have?" %><br>
  <%= f.number_field :tickets %><br><br>
  <%= f.submit %>
<% end %>