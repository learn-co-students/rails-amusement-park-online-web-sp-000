<%= form_for user, controller: 'users', action: 'create' do |f| %>
  <%= render partial: 'signin_elements', locals: {form: f} %>
  <%= f.label :height %>
  <%= f.number_field :height %><br>
  <%= f.label :happiness %>
  <%= f.number_field :happiness %><br>
  <%= f.label :nausea %>
  <%= f.number_field :nausea %><br>
  <%= f.label :tickets %>
  <%= f.label :admin %>
  <%= f.check_box :admin %>
  <%= f.number_field :tickets %><br>
  <%= f.submit "Create User"%>
<% end %>
