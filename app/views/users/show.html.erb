<div class="center">
  <% if flash[:notice] %>
    <%= flash[:notice] %>
  <% end %>
  <div class="info">

   Name: <%= @user.name %>
   <br>
   Tickets: <%= @user.tickets %>
   <br>
   Mood: <%= @user.mood %>
   <br>
   Height: <%= @user.height %>
   <br>
   <%= link_to "See attractions", attractions_path %>
   <br>
   <% if @user.admin == true %>
     ADMIN
   <% end %>
  </div>
</div>
