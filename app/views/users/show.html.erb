<% if flash[:notice] %>
    <div class="notice"><%= flash[:notice] %></div>
<% end %>
<p>Name: <%= @user.name %></p>
<% if @user.admin %>
<p>ADMIN</p>
<% else %>
<p>Tickets: <%= @user.tickets %></p>
<p>Mood: <%= @user.mood %></p>
<p>Height: <%= @user.height %></p>
<% end %>
<br>
<%= link_to "See attractions", controller: "attractions", action: "index" %>
<br>
<%= link_to "Log Out", controller: "sessions", action: "destroy" %>