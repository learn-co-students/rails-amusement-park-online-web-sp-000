<div class="center">
  <h2>My Profile</h2>

  <div class="info">
    <strong><%= "ADMIN" if @user.admin %></strong>
    <strong>Name: </strong><%= @user.name %><br>
    <strong>Tickets: </strong><%= @user.tickets %><br>
    <strong>Mood: </strong><%= @user.mood %><br>
    <strong>Height: </strong><%= @user.height %> inches
    <br><br>
    <a><%= link_to "See attractions", attractions_path %></a>
  </div>

</div>