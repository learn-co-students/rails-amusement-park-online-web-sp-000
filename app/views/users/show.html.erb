<% flash.each do |type, msg| %>
  <div>
    <%= msg %>
  </div>
<% end %>

<h1>Welcome <%= @user.name %></h1>
<% if !@user.admin%>
<p>Mood: <%= @user.mood %></p>
<% end %>
<p>Tickets: <%= @user.tickets %></p>
<p>Nausea: <%= @user.nausea %></p>
<p>Height: <%= @user.height %></p>
<% if @user.admin %>
<p>ADMIN</p>
<% end %>
<%= link_to "See attractions", attractions_path %>
 <p><%= link_to 'Log Out', logout_path %></p>
