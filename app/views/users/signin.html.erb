<%= form_for :user do |f| %>
    <%= f.label :name %><br>
    <%= f.collection_select :name, User.all, :name, :name, prompt: "Please select your username from this dropdown" %><br>
    <%= f.label :password %><br>
    <%= f.password_field :password %><br>

    <br />

    <%= f.submit "Sign In" %>
  <% end %>