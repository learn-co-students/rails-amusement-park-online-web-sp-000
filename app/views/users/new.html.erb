<h1>New User</h1>
<%= form_for @user do |f|%>
  <p>Name: <%= f.text_field :name %></p>
  <p>Height: <%= f.number_field :height %></p>
  <p>Happiness: <%= f.number_field :happiness %></p>
  <p>Nausea: <%= f.number_field :nausea %></p>
  <p>Tickes: <%= f.number_field :tickets %></p>
  <p>Admit: <%= f.check_box :admin, {}, true%></p>
  <p>Password: <%= f.password_field :password %></p>
  <%= f.submit :'Create User'%>
<% end %>
