<h1>Login form</h1>
<%= form_with model: @user do |f| %> 
    <%= f.text_field :name %>
    <%= f.text_field :height %>
    <%= f.text_field :happiness %>
    <%= f.text_field :nausea %>
    <%= f.text_field :tickets %>
    <%= f.check_box :admin , id: :user_admin %>
    <%= f.text_field :password %>
    <%= f.submit %>
<% end %>