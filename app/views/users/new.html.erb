<%= form_for @user do |f| %>
  <%= f.text_field :name %>
  <%= f.number_field :height %>
  <%= f.number_field :happiness %>
  <%= f.number_field :nausea %>
  <%= f.number_field :tickets %>
  <%= f.password_field :password %>
  Admin? <%= f.check_box :admin, option= {:class => "user_admin"} %>
  <%= f.submit %>
<% end %>
