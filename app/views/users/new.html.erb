<h1>Signup</h1>

<%= form_for @user do |f| %>
  <label>Name</label>
  <%= f.text_field :name %><br>
  <label>Height</label>
  <%= f.text_field :height %><br>
  <label>Happiness</label>
  <%= f.text_field :happiness %><br>
  <label>Nausea</label>
  <%= f.text_field :nausea %><br>
  <label>Tickets</label>
  <%= f.text_field :tickets %><br>
  <label>Password</label>
  <%= f.password_field :password %><br>
  <label>Admin?</label>
  <%= f.check_box :admin %>

  <%= f.submit %>
<% end %>
