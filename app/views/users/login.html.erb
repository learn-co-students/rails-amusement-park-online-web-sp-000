<h1>Login or <%= link_to "Sign Up", new_user_path %>!</h1>
<%= form_with(model: @user, url: signin_path) do | f | %>
    <%= f.label :user_name %>
    <%= f.collection_select :user_name, User.all, :name, :name %> #"Add this to notes"
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    <div class="actions">
        <%= f.submit "Sign In" %>
    </div>
<% end %>